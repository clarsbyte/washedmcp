  "mcps": {
    "github-mcp": {
      "name": "Github MCP",
      "description": "The GitHub MCP Server connects AI tools directly to GitHub's platform. This gives AI agents, assistants, and chatbots the ability to read repositories and code files, manage issues and PRs, analyze code, and automate workflows.",
      "documentation": "https://github.com/github/github-mcp-server",
      "category": "development",
      "installation": {
        "primary_method": "npx",
        "methods": {
          "npx": {
            "package": "@modelcontextprotocol/server-github",
            "registry": "npmjs",
            "command_template": "npx -y {package}",
            "requirements": {
              "node": ">=18.0.0",
              "npm": ">=9.0.0"
            },
            "tested": true,
            "last_verified": "2025-12-15",
            "success_rate": 0.98
          },
          "npm": {
            "package": "@modelcontextprotocol/server-github",
            "registry": "npmjs",
            "install_command": "npm install {package}",
            "requirements": {
              "node": ">=18.0.0",
              "npm": ">=9.0.0"
            },
            "tested": true
          }
        }
      },
      "configuration": {
        "type": "stdio",
        "env_vars": {
          "required": [
            {
              "name": "GITHUB_PERSONAL_ACCESS_TOKEN",
              "description": "GitHub Personal Access Token with repo permissions",
              "validation": "^gh[ps]_[A-Za-z0-9_]{36,}$",
              "docs_url": "https://docs.github.com/en/authentication/keeping-your-account-and-data-secure/creating-a-personal-access-token",
              "prompt": "Enter your GitHub Personal Access Token (create at https://github.com/settings/tokens):"
            }
          ],
          "optional": []
        },
        "command_generation": {
          "windows": {
            "command": "npx",
            "args": ["-y", "@modelcontextprotocol/server-github"]
          },
          "unix": {
            "command": "npx",
            "args": ["-y", "@modelcontextprotocol/server-github"]
          }
        }
      },
      "health_check": {
        "method": "mcp_list_tools",
        "timeout_seconds": 10,
        "expected_tools": ["create_issue", "search_repositories", "create_pull_request"]
      },
      "capabilities": [
        "github-integration",
        "repository-management",
        "issue-tracking",
        "pull-requests",
        "code-analysis",
        "version-control"
      ],
      "tools": [
        {
          "name": "create_issue",
          "description": "Create a new GitHub issue in a repository",
          "parameters": [
            {
              "name": "repository",
              "type": "string",
              "required": true,
              "description": "Repository in owner/repo format"
            },
            {
              "name": "title",
              "type": "string",
              "required": true,
              "description": "Issue title"
            },
            {
              "name": "body",
              "type": "string",
              "required": false,
              "description": "Issue body/description"
            }
          ],
          "returns": "Created issue URL and number",
          "example": "create_issue(repository='user/repo', title='Bug found', body='Description')"
        },
        {
          "name": "search_repositories",
          "description": "Search for GitHub repositories",
          "parameters": [
            {
              "name": "query",
              "type": "string",
              "required": true,
              "description": "Search query string"
            }
          ],
          "returns": "List of matching repositories",
          "example": "search_repositories(query='machine learning')"
        },
        {
          "name": "create_pull_request",
          "description": "Create a new pull request",
          "parameters": [
            {
              "name": "repository",
              "type": "string",
              "required": true,
              "description": "Repository in owner/repo format"
            },
            {
              "name": "title",
              "type": "string",
              "required": true,
              "description": "Pull request title"
            },
            {
              "name": "head",
              "type": "string",
              "required": true,
              "description": "Branch containing changes"
            },
            {
              "name": "base",
              "type": "string",
              "required": true,
              "description": "Branch to merge into"
            }
          ],
          "returns": "Created pull request URL and number",
          "example": "create_pull_request(repository='user/repo', title='Add feature', head='feature-branch', base='main')"
        }
      ],
      "use_cases": [
        "managing GitHub repositories",
        "creating and tracking issues",
        "code review workflows",
        "repository search and analysis",
        "pull request management"
      ]
    },
    "playwright-mcp": {
      "name": "Playwright MCP",
      "description": "A Model Context Protocol server that provides browser automation capabilities using Playwright. This server enables LLMs to interact with web pages, take screenshots, generate test code, web scrapes the page and execute JavaScript in a real browser environment.",
      "documentation": "https://github.com/executeautomation/mcp-playwright",
      "category": "automation",
      "installation": {
        "primary_method": "npx",
        "methods": {
          "npx": {
            "package": "@executeautomation/playwright-mcp-server",
            "registry": "npmjs",
            "command_template": "npx -y {package}",
            "requirements": {
              "node": ">=18.0.0",
              "npm": ">=9.0.0"
            },
            "tested": true,
            "last_verified": "2025-12-18",
            "success_rate": 0.95,
            "notes": "May require additional browser downloads on first run"
          },
          "npm": {
            "package": "@executeautomation/playwright-mcp-server",
            "registry": "npmjs",
            "install_command": "npm install {package}",
            "requirements": {
              "node": ">=18.0.0",
              "npm": ">=9.0.0"
            },
            "tested": true
          }
        }
      },
      "configuration": {
        "type": "stdio",
        "env_vars": {
          "required": [],
          "optional": [
            {
              "name": "PLAYWRIGHT_HEADLESS",
              "description": "Run browser in headless mode",
              "default": "true",
              "validation": "^(true|false)$"
            }
          ]
        },
        "command_generation": {
          "windows": {
            "command": "npx",
            "args": ["-y", "@executeautomation/playwright-mcp-server"]
          },
          "unix": {
            "command": "npx",
            "args": ["-y", "@executeautomation/playwright-mcp-server"]
          }
        }
      },
      "health_check": {
        "method": "mcp_list_tools",
        "timeout_seconds": 15,
        "expected_tools": ["navigate", "screenshot", "click"]
      },
      "capabilities": [
        "web-scraping",
        "browser-automation",
        "screenshots",
        "web-testing",
        "javascript-execution",
        "page-interaction"
      ],
      "tools": [
        {
          "name": "navigate",
          "description": "Navigate browser to a specified URL",
          "parameters": [
            {
              "name": "url",
              "type": "string",
              "required": true,
              "description": "Target URL to navigate to"
            }
          ],
          "returns": "Navigation success status",
          "example": "navigate(url='https://example.com')"
        },
        {
          "name": "screenshot",
          "description": "Take a screenshot of the current page or element",
          "parameters": [
            {
              "name": "selector",
              "type": "string",
              "required": false,
              "description": "CSS selector for specific element (optional, captures full page if not provided)"
            },
            {
              "name": "path",
              "type": "string",
              "required": false,
              "description": "File path to save screenshot"
            }
          ],
          "returns": "Screenshot image data or file path",
          "example": "screenshot(selector='#content', path='page.png')"
        },
        {
          "name": "click",
          "description": "Click on an element on the page",
          "parameters": [
            {
              "name": "selector",
              "type": "string",
              "required": true,
              "description": "CSS selector for the element to click"
            }
          ],
          "returns": "Click success status",
          "example": "click(selector='button#submit')"
        },
        {
          "name": "extract",
          "description": "Extract content from the page",
          "parameters": [
            {
              "name": "selector",
              "type": "string",
              "required": false,
              "description": "CSS selector for specific content (optional)"
            }
          ],
          "returns": "Extracted HTML or text content",
          "example": "extract(selector='.article-content')"
        }
      ],
      "use_cases": [
        "web scraping and data extraction",
        "browser automation",
        "taking screenshots of web pages",
        "automated web testing",
        "form filling and submission",
        "page interaction and navigation"
      ]
    },
    "firebase-mcp": {
      "name": "Firebase MCP",
      "description": "You can use the Firebase MCP server to give AI-powered development tools the ability to work with your Firebase projects and your app's codebase.",
      "documentation": "https://firebase.google.com/docs/ai-assistance/mcp-server#claude",
      "category": "cloud",
      "installation": {
        "primary_method": "npm",
        "methods": {
          "npm": {
            "package": "@google/genkit-mcp",
            "registry": "npmjs",
            "install_command": "npm install {package}",
            "requirements": {
              "node": ">=18.0.0",
              "npm": ">=9.0.0"
            },
            "tested": false,
            "notes": "Requires Firebase CLI to be installed separately"
          }
        }
      },
      "configuration": {
        "type": "stdio",
        "env_vars": {
          "required": [
            {
              "name": "FIREBASE_PROJECT_ID",
              "description": "Your Firebase project ID",
              "docs_url": "https://console.firebase.google.com/",
              "prompt": "Enter your Firebase project ID:"
            }
          ],
          "optional": [
            {
              "name": "FIREBASE_SERVICE_ACCOUNT",
              "description": "Path to Firebase service account JSON",
              "docs_url": "https://firebase.google.com/docs/admin/setup#initialize-sdk"
            }
          ]
        },
        "command_generation": {
          "windows": {
            "command": "node",
            "args": ["node_modules/@google/genkit-mcp/dist/index.js"]
          },
          "unix": {
            "command": "node",
            "args": ["node_modules/@google/genkit-mcp/dist/index.js"]
          }
        }
      },
      "capabilities": [
        "firebase-integration",
        "cloud-database",
        "authentication",
        "cloud-storage",
        "realtime-data"
      ],
      "tools": [
        {
          "name": "query_firestore",
          "description": "Query data from Firestore database",
          "parameters": [
            {
              "name": "collection",
              "type": "string",
              "required": true,
              "description": "Firestore collection name"
            },
            {
              "name": "query",
              "type": "object",
              "required": false,
              "description": "Query filters"
            }
          ],
          "returns": "Query results from Firestore",
          "example": "query_firestore(collection='users', query={'age': {'>=': 18}})"
        },
        {
          "name": "write_firestore",
          "description": "Write data to Firestore database",
          "parameters": [
            {
              "name": "collection",
              "type": "string",
              "required": true,
              "description": "Firestore collection name"
            },
            {
              "name": "data",
              "type": "object",
              "required": true,
              "description": "Data to write"
            }
          ],
          "returns": "Write confirmation with document ID",
          "example": "write_firestore(collection='users', data={'name': 'John', 'email': 'john@example.com'})"
        }
      ],
      "use_cases": [
        "cloud database operations",
        "user authentication",
        "real-time data synchronization",
        "cloud file storage",
        "Firebase project management"
      ]
    },
    "slack-mcp": {
      "name": "Slack",
      "description": "Send messages, manage channels, and access workspace history through Slack.",
