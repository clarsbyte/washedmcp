    "imagesorcery": {
      "name": "ImageSorcery",
      "description": "Provides powerful image manipulation capabilities including resizing, cropping, object detection, OCR text extraction, and finding objects based on text descriptions using Python with OpenCV and Ultralytics",
      "documentation": "https://github.com/sunriseapps/imagesorcery-mcp",
      "category": "image-processing",
      "installation": {
        "primary_method": "manual",
        "methods": {
          "manual": {
            "documentation_url": "https://github.com/sunriseapps/imagesorcery-mcp"
          }
        }
      },
      "configuration": {
        "type": "stdio",
        "env_vars": {
          "required": [],
          "optional": []
        }
      },
      "capabilities": [
        "image-processing",
        "image-manipulation",
        "object-detection",
        "ocr",
        "image-analysis"
      ],
      "tools": [
        {
          "name": "resize_image",
          "description": "Resize an image to specified dimensions",
          "parameters": [
            {
              "name": "image_path",
              "type": "string",
              "required": true,
              "description": "Path to the input image file"
            },
            {
              "name": "width",
              "type": "number",
              "required": true,
              "description": "Target width in pixels"
            },
            {
              "name": "height",
              "type": "number",
              "required": true,
              "description": "Target height in pixels"
            }
          ],
          "returns": "Path to the resized image",
          "example": "resize_image(image_path='photo.jpg', width=800, height=600)"
        },
        {
          "name": "crop_image",
          "description": "Crop an image to specified region",
          "parameters": [
            {
              "name": "image_path",
              "type": "string",
              "required": true,
              "description": "Path to the input image file"
            },
            {
              "name": "x",
              "type": "number",
              "required": true,
              "description": "X coordinate of crop region"
            },
            {
              "name": "y",
              "type": "number",
              "required": true,
              "description": "Y coordinate of crop region"
            },
            {
              "name": "width",
              "type": "number",
              "required": true,
              "description": "Width of crop region"
            },
            {
              "name": "height",
              "type": "number",
              "required": true,
              "description": "Height of crop region"
            }
          ],
          "returns": "Path to the cropped image",
          "example": "crop_image(image_path='photo.jpg', x=100, y=100, width=400, height=300)"
        },
        {
          "name": "detect_objects",
          "description": "Detect objects in an image using AI",
          "parameters": [
            {
              "name": "image_path",
              "type": "string",
              "required": true,
              "description": "Path to the input image file"
            }
          ],
          "returns": "List of detected objects with bounding boxes and confidence scores",
          "example": "detect_objects(image_path='photo.jpg')"
        },
        {
          "name": "extract_text",
          "description": "Extract text from an image using OCR",
          "parameters": [
            {
              "name": "image_path",
              "type": "string",
              "required": true,
              "description": "Path to the input image file"
            }
          ],
          "returns": "Extracted text content",
          "example": "extract_text(image_path='document.jpg')"
        }
      ],
      "use_cases": [
        "image resizing and manipulation",
        "object detection in images",
        "OCR and text extraction from images",
        "image cropping and editing",
        "automated image processing workflows"
      ]
