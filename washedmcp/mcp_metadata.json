          "unix": {
            "command": "node",
            "args": ["node_modules/slack-mcp-server/dist/index.js"]
          }
        }
      },
      "capabilities": [
        "slack-integration",
        "team-communication",
        "message-sending",
        "channel-management",
        "workspace-history"
      ],
      "tools": [
        {
          "name": "send_message",
          "description": "Send a message to a Slack channel or user",
          "parameters": [
            {
              "name": "channel",
              "type": "string",
              "required": true,
              "description": "Channel ID or name (e.g., #general or C1234567890)"
            },
            {
              "name": "text",
              "type": "string",
              "required": true,
              "description": "Message text to send"
            }
          ],
          "returns": "Message timestamp and channel confirmation",
          "example": "send_message(channel='#general', text='Hello team!')"
        },
        {
          "name": "list_channels",
          "description": "List all channels in the workspace",
          "parameters": [],
          "returns": "List of channel names and IDs",
          "example": "list_channels()"
        },
        {
          "name": "get_channel_history",
          "description": "Get message history from a channel",
          "parameters": [
            {
              "name": "channel",
              "type": "string",
              "required": true,
              "description": "Channel ID or name"
            },
            {
              "name": "limit",
              "type": "number",
              "required": false,
              "description": "Number of messages to retrieve (default: 100)"
            }
          ],
          "returns": "List of messages from the channel",
          "example": "get_channel_history(channel='#general', limit=50)"
        }
      ],
      "use_cases": [
        "sending messages to Slack channels",
        "team communication and notifications",
        "channel management",
        "retrieving workspace message history",
        "Slack bot integration"
      ]
    },
